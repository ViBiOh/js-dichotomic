{
  "name": "js-dichotomic",
  "version": "1.0.0",
  "description": "Javascript Utils Dichotomic/Binary search/sort",
  "main": "index.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint src/",
    "pretest": "npm run eslint",
    "test": "./node_modules/.bin/nyc --reporter html --reporter text --reporter lcov --require babel-core/register ./node_modules/.bin/ava",
    "coverage": "./node_modules/.bin/codecov -f ./coverage/lcov.info",
    "build:uglify": "./node_modules/.bin/babel ./src/dichotomic.js | ./node_modules/.bin/uglifyjs -o index.js -c -mt --preamble //${npm_package_name}@${npm_package_version}",
    "build": "npm run test && npm run build:uglify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-dichotomic.git"
  },
  "keywords": [
    "dichotomic",
    "search",
    "sort"
  ],
  "author": "Vincent Boutour",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-dichotomic/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-dichotomic",
  "devDependencies": {
    "ava": "latest",
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-preset-es2015": "latest",
    "codecov": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "nyc": "latest",
    "uglify-js": "latest"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
